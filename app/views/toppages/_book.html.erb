<div class='user_container row'>
  <% books.each do |book| %>
    
      <div class='col-md-3 user_box'>
        <div class='book_info_img'>
          <%= image_tag book.image_url %>
        </div>
        <h5 class='pt-4'><%= book.title %></h5>
      </div>
      
   
  <% end %>
</div>
<div class='more_users row'>
  <div class='offset-sm-10 col-sm-2'>
    <%= link_to "もっとみる　\uf101", books_path, class: 'fas' %>
  </div>
</div>