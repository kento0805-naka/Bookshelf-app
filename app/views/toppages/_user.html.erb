<div class='user_container row'>
  <% users.each do |user| %>
    
      <div class='col-md-3 user_box'>
        <div class='user_info_img'>
          <% if user.profile_img? %>
            <%= link_to (image_tag(user.profile_img.thumb.url)), user_path(user.id)%>
          <% else %>
            <i class="far fa-user-circle "></i>
          <% end %>
        </div>
        <h5><%= user.name %></h5>
      </div>
      
   
  <% end %>
</div>
<%= render 'search_more' %>