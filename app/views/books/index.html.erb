<div class='books_page'>
  <div>
    <div class='new_user_title'>新規登録本</div>
    <hr></hr>
    <div class='book_content'>
      <div class='book_boxs'>
        <div class='book_container_index row'>
          <% @books.each do |book| %>
            
              <div class='col-lg-3 col-md-4 book_box'>
                <div class='book_info_img'>
                
                    <%= link_to (image_tag(book.image_url)), book_path(book.id)%>
                 
                </div>
                <h5><%= book.title %></h5>
              </div>
              
           
          <% end %>
      </div>
    </div>
  </div>
</div>